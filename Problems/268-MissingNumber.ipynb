    "class Solution(object):\n",
    "    def missingNumber(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        n=len(nums)\n",
    "        totalSum=n*(n+1)/2\n",
    "        \n",
    "        currentSum=sum(nums)\n",
    "        return (totalSum-currentSum)"
   ]
